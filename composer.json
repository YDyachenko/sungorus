{
    "name": "ydyachenko/sungorus",
    "description": "Password manager",
    "keywords": [
        "zf2",
        "passwords",
        "manager"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "container-interop/container-interop": "^1.2",
        "neilime/zf2-twb-bundle": "^3.0",
        "twbs/bootstrap": "^3.3",
        "zendframework/zend-authentication": "^2.6.0",
        "zendframework/zend-crypt": "^3.2.0",
        "zendframework/zend-db": "^2.9.0",
        "zendframework/zend-escaper": "^2.6.0",
        "zendframework/zend-eventmanager": "^3.2.0",
        "zendframework/zend-form": "^2.11.0",
        "zendframework/zend-http": "^2.8.0",
        "zendframework/zend-hydrator": "^2.3",
        "zendframework/zend-inputfilter": "^2.8.1",
        "zendframework/zend-math": "^3.0",
        "zendframework/zend-modulemanager": "^2.8.2",
        "zendframework/zend-mvc": "^3.1.1",
        "zendframework/zend-mvc-console": "^1.2",
        "zendframework/zend-mvc-plugin-identity": "^1.1",
        "zendframework/zend-servicemanager": "^3.3.0",
        "zendframework/zend-session": "^2.8.5",
        "zendframework/zend-stdlib": "^3.1",
        "zendframework/zend-view": "^2.10.0",
        "zfcampus/zf-development-mode": "^3.1.0"
    },
    "require-dev": {
        "ydyachenko/db-queries-toolbar": "^0.2.0",
        "zendframework/zend-coding-standard": "^1.0",
        "zendframework/zend-developer-tools": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "chmod a+w data/cache",
            "chmod a+w data/favicons"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
